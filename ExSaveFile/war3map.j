set align=2
elseif vert==JN_TEXT_JUSTIFY_BOTTOM then
set align=4
endif
if horz==JN_TEXT_JUSTIFY_LEFT then
set align=align+8
elseif horz==JN_TEXT_JUSTIFY_CENTER then
set align=align+16
elseif horz==JN_TEXT_JUSTIFY_RIGHT then
set align=align+32
endif
call DzFrameSetTextAlignment(frame,align)
endfunction
