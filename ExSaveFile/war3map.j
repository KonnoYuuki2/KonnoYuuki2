if GetUnitTypeId(u)=='HR04' then
if GetRandomInt(1,2)==1 then
call SetUnitAnimationByIndex(GetAttacker(),17)
set GutsAttack=false
else
call SetUnitAnimationByIndex(GetAttacker(),1)
set GutsAttack=true
endif
endif
